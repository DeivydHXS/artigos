{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "cy:run": "npx cypress run",
        "cy:api": "npx cypress run --spec cypress/e2e/api/user/*.cy.js",
        "cy:login": "npm run cy:run -- --spec cypress/e2e/api/user/login.cy.js",
        "cy:register": "npm run cy:run -- --spec cypress/e2e/api/user/register.cy.js",
        "cy:edit": "npm run cy:run -- --spec cypress/e2e/api/user/edit.cy.js",
        "cy:delete": "npm run cy:run -- --spec cypress/e2e/api/user/delete.cy.js",
        "cy:special": "npm run cy:run -- --spec cypress/e2e/api/special-cases/special-cases.cy.js"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "cypress": "^14.5.1",
        "laravel-vite-plugin": "^1.2.0",
        "tailwindcss": "^4.0.0",
        "vite": "^6.2.4"
    }
}
